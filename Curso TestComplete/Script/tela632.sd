procedure gravar;
begin

    Sys.Process('PCSIS632').VCLObject('frmPcinf771').VCLObject('WallpaperRotinaPC1').VCLObject('pnlForm').VCLObject('pnlRodape').VCLObject('btnEntrar2').Click;  
    Sys.Process('PCSIS632').Window('TMessageForm', 'Confirmação', 1).VCLObject('Yes')
    
end;

////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
procedure confirmacao;
begin
  
    if Sys.Process('PCSIS632').Window('TMessageForm', 'Confirmação', 1).Exists then
      begin
        log.Checkpoint('Registro Gravado');
        Sys.Process('PCSIS632').Window('TMessageForm', 'Confirmação', 1).VCLObject('Yes').Click;
      end
    else
        log.Error('Mensagem de confirmação não apareceu');
      
end;

////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
procedure impressao;
begin
  
    if Sys.Process('PCSIS632').Window('TMessageForm', 'Confirmação', 1).Exists then
      begin
        log.Checkpoint('Registro Impresso');
        Sys.Process('PCSIS632').Window('TMessageForm', 'Confirmação', 1).VCLObject('No').Click;
      end
    else
        log.Error('Mensagem de impressão não apareceu');
      
end;